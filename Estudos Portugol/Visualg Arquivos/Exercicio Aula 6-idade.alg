algoritmo "Idade"
var
   ano_atual, ano_nascimento, idade: Inteiro
inicio
      Escreval("Em que ano nós estamos: ")
      Leia(ano_atual)
      Escreval ("Em que ano eu nasci ?")
      Leia(ano_nascimento)
      idade <- ano_atual - ano_nascimento
      Escreval ("Minha idade será ", idade)
fimalgoritmo
