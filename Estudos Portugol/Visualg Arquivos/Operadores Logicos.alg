algoritmo "Operadores Logicos"
var
   A, B, C: inteiro
inicio
      A <- 2
      B <- 3
      C <- 5
      Escreval ((A=B) ou (C>A))
      Escreval ((1%A) e (C>A)) // "E"  so da verdadeiro se os 2 forem verdadeiro
      Escreval ((2>=A) ou (C=A)) // "ou" so da falso se os 2 forem falso
fimalgoritmo
